{
  "name": "my-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyzer": "ng build --configuration analyzer --stats-json && webpack-bundle-analyzer dist/stats.json",
    "dll": "webpack --config webpack.dll.js",
    "fix:eslint": "eslint src --fix --ignore-path .eslintignore",
    "lint:eslint": "eslint src --ignore-path .eslintignore",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "commitlint": "npx commitlint --config commitlint.config.js -e -V"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/common": "^19.0.3",
